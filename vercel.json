{
  "headers": [
    {
      "source": "/embed",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors *"
        },
        {
          "key": "Permissions-Policy",
          "value": "display-capture=*, fullscreen=*"
        }
      ]
    },
    {
      "source": "/embed/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors *"
        },
        {
          "key": "Permissions-Policy",
          "value": "display-capture=*, fullscreen=*"
        }
      ]
    }
  ]
}

